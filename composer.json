{
    "name": "swisscode/newt4news",
    "type": "typo3-cms-extension",
    "description": "Extension with the Newt-Provider for News",
    "homepage": "https://extensions.typo3.org/extension/newt4news",
    "support": {
        "docs": "https://docs.typo3.org/p/swisscode/newt4news/main/en-us/",
        "issues": "https://github.com/juergenfurrer/newt4news-typo3-ext/issues",
        "source": "https://github.com/juergenfurrer/newt4news-typo3-ext"
    },
    "authors": [
        {
            "name": "Juergen Furrer",
            "role": "Developer"
        }
    ],
    "license": "GPL-2.0-or-later",
    "require": {
        "typo3/cms-core": "^11.5 || ^12.4",
        "swisscode/newt": "^3.0.0",
        "georgringer/news": "^11.0.0"
    },
    "autoload": {
        "psr-4": {
            "Swisscode\\Newt4News\\": "Classes"
        }
    },
    "extra": {
        "typo3/cms": {
            "extension-key": "newt4news"
        }
    }
}
